                            <div class="carousel-item">
                                <img src="~/assets/img/inmuebles/dpto_4.jpg" alt="" class="card-img-top">
                            </div>
                            <div class="carousel-item">
                                <img src="~/assets/img/inmuebles/dpto_5.jpg" alt="" class="card-img-top">
