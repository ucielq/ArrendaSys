
@{
    ViewBag.Title = "InmueblesInmobiliariaView";
}


<section id="header" class="jumbotron text-center">
    <h1 class="display-3">Mis Inmuebles</h1>
    <p class="lead">Actualmente, estos son sus inmuebles en Arrendasys.</p>
    <button type="button" class="btn btn-success">
        <i class="fas fa-plus"></i> Agregar nuevo inmueble
    </button>
</section>

<section id="gallery">
    <div class="container">
        <div class="row">
            <div class="col-lg-4 mb-4">
                <div class="card">
                    <div id="carouselExampleControls" class="carousel slide" data-ride="carousel">
                        <div class="carousel-inner">
                            <div class="carousel-item active">
                                <img src="~/assets/img/inmuebles/casa_1.jpg" alt="" class="card-img-top">
                            </div>
                            <div class="carousel-item">
                                <img src="~/assets/img/inmuebles/casa_2.jpg" alt="" class="card-img-top">
                            </div>
                            <div class="carousel-item">
                                <img src="~/assets/img/inmuebles/casa_3.jpg" alt="" class="card-img-top">
                            </div>
                            <div class="carousel-item">
                                <img src="~/assets/img/inmuebles/casa_4.jpg" alt="" class="card-img-top">
                            </div>
                            <div class="carousel-item">
                                <img src="~/assets/img/inmuebles/casa_5.jpg" alt="" class="card-img-top">
                            </div>
                        </div>
                        <a class="carousel-control-prev" href="#carouselExampleControls" role="button" data-slide="prev">
                            <span class="carousel-control-prev-icon" aria-hidden="true"></span>
                            <span class="sr-only">Previous</span>
                        </a>
                        <a class="carousel-control-next" href="#carouselExampleControls" role="button" data-slide="next">
                            <span class="carousel-control-next-icon" aria-hidden="true"></span>
                            <span class="sr-only">Next</span>
                        </a>
                    </div>
                    <div class="card-body">
                        <h5 class="card-title">Alzaga 5123 - Chacras de Coria, Luján de Cuyo</h5>
                        <p class="card-text">
                            Casa.
                        <p><i class="fas fa-ruler-combined"></i>  650m².
                        <p><i class="fas fa-home"></i>  220m².
                        <p><i class="fas fa-bed"></i>  3 Dormitorios.
                        <p><i class="fas fa-shower"></i>  2 Baños.
                        <p>
                            <i class="fas fa-warehouse"></i>  1 Cochera.
                        </p>
                        <a href="" class="btn btn-outline-success btn-sm">Editar</a>
                        <a href="" class="btn btn-outline-danger btn-sm">Eliminar</a>
                    </div>
                </div>
            </div>
            <div class="col-lg-4 mb-4">
                <div class="card">
                    <div id="carouselExampleControls2" class="carousel slide" data-ride="carousel">
                        <div class="carousel-inner">
                            <div class="carousel-item active">
                                <img src="~/assets/img/inmuebles/dpto_1.jpg" alt="" class="card-img-top">
